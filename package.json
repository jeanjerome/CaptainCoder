{
	"name": "captaincoder",
	"displayName": "CaptainCoder",
	"description": "CaptainCoder is a privacy-focused, locally-run code completion extension for VSCode that supports any Ollama model and is open-source.",
	"version": "1.1.0",
	"icon": "icon.png",
	"publisher": "jeanjerome",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jeanjerome/captaincoder/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jeanjerome/captaincoder"
	},
	"engines": {
		"vscode": "^1.89.0"
	},
	"categories": [
		"AI",
		"Machine Learning",
		"Extension Packs",
		"Programming Languages"
	],
	"keywords": [
		"llama",
		"ollama",
		"gpt",
		"coding",
		"autocomplete",
		"open source",
		"assistant",
		"ai",
		"llm"
	],
	"activationEvents": [
		"onCommand:captaincoder.generateCompletion",
		"onCommand:captaincoder.configureParameters",
		"onCommand:captaincoder.refreshModelList",
		"onCommand:captaincoder.downloadModel",
		"onCommand:captaincoder.deleteModel",
		"onCommand:captaincoder.showModelManager"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "captaincoder.generateCompletion",
				"title": "CaptainCoder: Generate Completion"
			},
			{
				"command": "captaincoder.configureParameters",
				"title": "CaptainCoder: Configure Parameters"
			},
			{
				"command": "captaincoder.refreshModelList",
				"title": "CaptainCoder: Refresh Model List"
			},
			{
				"command": "captaincoder.downloadModel",
				"title": "CaptainCoder: Download Model"
			},
			{
				"command": "captaincoder.deleteModel",
				"title": "CaptainCoder: Delete Model"
			},
			{
				"command": "captaincoder.showModelManager",
				"title": "CaptainCoder: Show Model Manager"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "modelTreeView",
					"name": "Ollama Models"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "CaptainCoder",
			"properties": {
				"captaincoder.model": {
					"type": "string",
					"default": "qwen2:7b",
					"description": "The model to use for generating completions."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^14.18.63",
		"@types/node-fetch": "^2.6.11",
		"typescript": "^4.3.5",
		"vscode": "^1.1.37"
	},
	"dependencies": {
		"node-fetch": "^2.7.0"
	}
}
